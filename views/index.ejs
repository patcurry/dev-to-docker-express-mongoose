<!DOCTYPE html>
<html lang="en">

<head>
  <title>My Node App</title>
</head>

<body>
  <h1>My Node App</h1>
  <form method="post" action="/item/add">
    <label for="name">Name</label>
    <input type="text" name="name">
    <input type="submit" value="Add">
  </form>
    <h4>names:</h4>
  <ul>
    <% items.forEach(function(item) { %>
    <li>
      <%= item.name %>
    </li>
    <% }); %>
  </ul>
</body>

</html>
